<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="juranoaa.com.eventbindingsample" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <activity
            android:name=".AAMainActivity_"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".AAClickSampleActivity_"
            android:label="AAClickSampleActivity" >
        </activity>
        <activity
            android:name=".ClickSampleActivity"
            android:label="ClickSampleActivity" >
        </activity>
        <activity
            android:name=".AALongClickSampleActivity_"
            android:label="AALongClickSampleActivity">
        </activity>
        <activity
            android:name=".LongClickSampleActivity"
            android:label="LongClickSampleActivity">
        </activity>
        <activity
            android:name=".AATouchSampleActivity_"
            android:label="AATouchSampleActivity">
        </activity>
        <activity
            android:name=".TouchSampleActivity"
            android:label="TouchSampleActivity">
        </activity>

    </application>

</manifest>
