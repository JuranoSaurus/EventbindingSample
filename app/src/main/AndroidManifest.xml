<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.juranoaa.eventbindingsample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".AAMainActivity_"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".clickandtouch.AAClickSampleActivity_"
            android:label="AAClickSampleActivity"></activity>
        <activity
            android:name=".clickandtouch.ClickSampleActivity"
            android:label="ClickSampleActivity"></activity>
        <activity
            android:name=".clickandtouch.AALongClickSampleActivity_"
            android:label="AALongClickSampleActivity"></activity>
        <activity
            android:name=".clickandtouch.LongClickSampleActivity"
            android:label="LongClickSampleActivity"></activity>
        <activity
            android:name=".clickandtouch.AATouchSampleActivity_"
            android:label="AATouchSampleActivity"></activity>
        <activity
            android:name=".clickandtouch.TouchSampleActivity"
            android:label="TouchSampleActivity"></activity>
        <activity
            android:name=".adapterview.AAItemClickSampleActivity_"
            android:label="AAItemClickActivity"></activity>
        <activity
            android:name=".adapterview.ItemClickSampleActivity"
            android:label="ItemClickSampleActivity"></activity>
        <activity
            android:name=".adapterview.AAItemSelectSampleActivity_"
            android:label="AAItemSelectSampleActivity"></activity>
        <activity
            android:name=".adapterview.ItemSelectSampleActivity"
            android:label="AAItemSelectSampleActivity"></activity>
        <activity
            android:name=".seekbar.AASeekBarSampleActivity_"
            android:label="AASeekBarSampleActivity"></activity>
        <activity
            android:name=".seekbar.SeekBarSampleActivity"
            android:label="SeekBarSampleActivity"></activity>
        <activity
            android:name=".text.AATextChangeSampleActivity_"
            android:label="AATextChangeSampleActivity"></activity>
        <activity
            android:name=".text.TextChangeSampleActivity"
            android:label="TextChangeSampleActivity"></activity>
    </application>

</manifest>
